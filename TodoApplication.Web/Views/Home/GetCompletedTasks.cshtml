@model TodoTasksViewModel

<div class="row">
    <div class="col-md-6">
        <h3 class="ms-5">Completed Tasks</h3>
    </div>
</div>


@foreach (var p in Model.TodoTasks)
{
        <partial name="TodoTaskSummary" model="p" />
}

<div page-model="@Model.PagingInfo" page-action="GetCompletedTasks" page-classes-enabled="true"
     page-class="btn" page-class-normal="btn-outline-dark"
     page-class-selected="btn-primary" 
     class="btn-group pull-right m-1">
</div>